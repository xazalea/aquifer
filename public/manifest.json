{
  "name": "Aquifer - Android VM in Browser",
  "short_name": "Aquifer",
  "description": "Run Android OS and APKs directly in your browser",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#3DDC84",
  "orientation": "any",
  "icons": [
    {
      "src": "/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "categories": ["utilities", "developer"],
  "screenshots": [],
  "shortcuts": [
    {
      "name": "Start VM",
      "short_name": "Start",
      "description": "Quickly start the Android VM",
      "url": "/?action=start",
      "icons": [{ "src": "/icon.png", "sizes": "192x192" }]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "apk",
          "accept": [".apk", "application/vnd.android.package-archive"]
        }
      ]
    }
  }
}

